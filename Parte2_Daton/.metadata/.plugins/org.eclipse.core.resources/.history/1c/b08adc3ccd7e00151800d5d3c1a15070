package part1;
 class tCartaPalo{
		

		    enum  tPalo { CLUB, DIAMOND, SPADE, HEART};
		    enum  tCarta{ A,K,Q,J,T,NINE,EIGHT,SEVEN,SIX,FIVE,FOUR,THREE,TWO}
		
		 tPalo _palo;
		 tCarta _carta;
		 
		 public tCartaPalo(char unaCarta, char unPalo)
		 {
			switch (unPalo)
			{
			 case 'c': _palo=tPalo.CLUB; break;
			 case 'd': _palo=tPalo.DIAMOND; break;
			 case 's': _palo=tPalo.SPADE; break;
			 case 'h': _palo=tPalo.HEART;break;
			 
			}
			
			switch (UnaCarta)
			{
			case 'A': return 14; 
			case 'K': return 13; 
			case 'Q': return 12; 
			case 'J': return 11; 
			case 'T': return 10; 
			case '9': return 9; 
			case '8': return 8; 
			case SEVEN: return 7; 
			case SEIS: return 6; 
			case CINCO: return 5; 
			case CUATRO: return 4; 
			case TRES: return 3; 
			case DOS: return 2; 
			default: return -1;
		
			}
			
			 
			 	
		 }
		 
		 public int dameValor() 
		{
			switch(_carta)
			{
			case A: return 14; 
			case K: return 13; 
			case Q: return 12; 
			case J: return 11; 
			case T: return 10; 
			case NINE: return 9; 
			case EIGHT: return 8; 
			case SEVEN: return 7; 
			case SIX: return 6; 
			case FIVE: return 5; 
			case FOUR: return 4; 
			case THREE: return 3; 
			case TWO: return 2; 
			default: return -1;
			}
			
		}
		 
		 public String damePalo()
			{
			 
			}
		
	}
	

		
	
