package part1;


import java.util.ArrayList;

//main
public class parte1
{
	public enum tPalo{ CLUB, DIAMOND, SPADE, HEART};
	public enum tCarta{ A,K,Q,J,T,NINE,EIGHT,SIETE,SEIS,CINCO,CUATRO,TRES,DOS}
	public class tCartaPalo{
		public tCarta _carta;
		public tPalo _palo;
		public int dameValor() throws Exception
		{
			switch(this._carta)
			{
			case A: return 14; break;
			case K: return 13; break;
			case Q: return 12; break;
			case J: return 11; break;
			case T: return 10; break;
			case NINE: return 9; break;
			case EIGHT: return 8; break;
			case SIETE: return 7; break;
			case SEIS: return 6; break;
			case CINCO: return 5; break;
			case CUATRO: return 4; break;
			case TRES: return 3; break;
			case DOS: return 2; break;
			
			
			
			}
		}
	}
	public class tListaCartaPalo{
		public ArrayList<tCartaPalo> _unaLista;
		
	}
public static void main(String [] args)
	{
	
	
	}
}