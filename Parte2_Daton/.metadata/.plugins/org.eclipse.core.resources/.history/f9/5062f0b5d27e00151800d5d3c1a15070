package part1;


import java.util.*;


class clasePrinc
{
	ArrayList<tCartaPalo> listaCartas;
	
	
public void main (String[] args)
{
	char[] unaCadena=args[0].toCharArray();
	
	int i=0;
	while (i<unaCadena.length-1)
	{
		listaCartas.addCarta (new tCartapalo(unaCadena[i],unaCadena[i+1]));
		i=i+2;
	}
	

	
}

}