package part1;
 class tCartaPalo{
		

		    enum  tPalo { CLUB, DIAMOND, SPADE, HEART};
		    enum  tCarta{ A,K,Q,J,T,NINE,EIGHT,SEVEN,SIX,FIVE,FOUR,THREE,TWO}
		
		 tPalo _palo;
		 tCarta _carta;
		 
		 public tCartaPalo(char unaCarta, char unPalo)
		 {
			switch (unPalo)
			{
			 case 'c': _palo=tPalo.CLUB; break;
			 case 'd': _palo=tPalo.DIAMOND; break;
			 case 's': _palo=tPalo.SPADE; break;
			 case 'h': _palo=tPalo.HEART;break;
			 
			}
			
			switch (UnaCarta)
			{
			case 'A': _carta=A;break; 
			case 'K': _carta=K;break; 
			case 'Q': _carta=Q;break; 
			case 'J': _carta=J;break; 
			case 'T': _carta=T;break; 
			case '9': _carta=NINE;break;
			case '8': _carta=EIGHT;break;
			case '7': _carta=SEVEN;break; 
			case '6': _carta=SIX;break; 
			case '5': _carta=FIVE;break; 
			case '4': _carta=FOUR;break;
			case '3': _carta=THREE;break;
			case '2': _carta=TWO;break; 
			default: return -1;
		
			}
			
			 
			 	
		 }
		 
		 public int dameValor() 
		{
			switch(_carta)
			{
			case A: return 14; 
			case K: return 13; 
			case Q: return 12; 
			case J: return 11; 
			case T: return 10; 
			case NINE: return 9; 
			case EIGHT: return 8; 
			case SEVEN: return 7; 
			case SIX: return 6; 
			case FIVE: return 5; 
			case FOUR: return 4; 
			case THREE: return 3; 
			case TWO: return 2; 
			default: return -1;
			}
			
		}
		 
		 public String damePalo()
			{
			 
			}
		
	}
	

		
	
