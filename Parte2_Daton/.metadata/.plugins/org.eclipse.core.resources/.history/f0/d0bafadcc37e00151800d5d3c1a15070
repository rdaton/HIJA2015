package part1;
 class tCartaPalo{
		 
		public enum tPalo{ CLUB, DIAMOND, SPADE, HEART};
		public  enum tCarta{ A,K,Q,J,T,NINE,EIGHT,SIETE,SEIS,CINCO,CUATRO,TRES,DOS}

		  tPalo _palo;
		  tCarta _carta;
		  
		
		 
		 public tCartaPalo(char unaCarta, char unPalo)
		 {
			// switch (unPalo)
			// {
			// case 'c': this._palo=DIAMOND; break;
			 
			 
			 }
			 
		 }
		 
		 public int dameValor() 
		{
			switch(_carta)
			{
			case A: return 14; 
			case K: return 13; 
			case Q: return 12; 
			case J: return 11; 
			case T: return 10; 
			case NINE: return 9; 
			case EIGHT: return 8; 
			case SIETE: return 7; 
			case SEIS: return 6; 
			case CINCO: return 5; 
			case CUATRO: return 4; 
			case TRES: return 3; 
			case DOS: return 2; 
			default: return -1;
			}
		
	}
	
}
		
	
